{
  "name": "travel-journal",
  "homepage": "http://milliorn.github.io/Travel-Journal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "4.3.1",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css-minify": "npx tailwindcss -o ./build/build.css --minify",
    "css-update": "npm install -D tailwindcss@latest postcss@latest autoprefixer@latest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "10.4.7",
    "gh-pages": "3.2.3",
    "postcss": "8.4.13",
    "tailwindcss": "3.0.24"
  }
}
